{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@4.2.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "font-awesome-5-css@5.4.1",
      "destination": "wwwroot/lib/font-awesome-5-css/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-solid-900.woff2"
      ]
    },
    {
      "provider": "unpkg",
      "library": "sweetalert2@8.2.3",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "dist/sweetalert2.css",
        "dist/sweetalert2.min.css",
        "dist/sweetalert2.all.js",
        "dist/sweetalert2.all.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "modernizr@2.6.2",
      "destination": "wwwroot/lib/modernizr/"
    },
    {
      "provider": "cdnjs",
      "library": "waypoints@4.0.0",
      "destination": "wwwroot/lib/waypoints/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-easing@1.3",
      "destination": "wwwroot/lib/jquery-easing/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/lib/jquery-validate/"
    }
  ]
}